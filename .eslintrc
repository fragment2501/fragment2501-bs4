{
  "env": {
    "browser": true,
    "es6": true,
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended" 
  ],
  "globals": {
    "Vue": true,
    "axios": true
  },
  "rules": {
    "eqeqeq": "off",
    "curly": "error",
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "semi": "error",
    "vue/html-indent": ["error", 2],
    "vue/script-indent": ["error", 2],
    "vue/html-quotes": ["error", "double"]
  }
}
